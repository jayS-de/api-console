#%RAML 0.8
title: My API
baseUri: http://localhost

/resource:
  get:
    queryParameters:
      display:
        type: boolean
        default: false
        example: false
      type:
        type: string
        default: hola
        example: lalala
      other:
        type: string
        enum: [uno, dos, tres]
        default: uno
        example: dos
      page:
        type: integer
        default: 0
      active:
        type: boolean
        example: true
        default: false
